{
    "name": "hkulekci/qdrant",
    "description": "PHP Client for Qdrant",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^3.0",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.4@dev",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "openai-php/client": "^0.3.5",
        "hkulekci/cohere": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Qdrant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qdrant\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Haydar Kulekci",
            "email": "haydarkulekci@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --display-warnings --display-deprecations"
    }
}
