{
    "name": "hkulekci/qdrant",
    "description": "PHP Client for Qdrant",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "webmozart/assert": "^1.11",
        "symfony/polyfill-php73": "1.x-dev",
        "symfony/polyfill-php80": "1.x-dev",
        "guzzlehttp/psr7": "1.9.x-dev",
        "http-interop/http-factory-guzzle": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Qdrant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qdrant\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Haydar Kulekci",
            "email": "haydarkulekci@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --display-warnings --display-deprecations",
        "coverage": "vendor/bin/phpunit --display-warnings --display-deprecations --coverage-html coverage/"
    }
}
